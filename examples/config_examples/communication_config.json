{
  "mcpServers": {
    "slack": {
      "command": "mcp-server-slack",
      "args": [],
      "env": {
        "SLACK_TOKEN": "your_slack_token"
      }
    },
    "gmail": {
      "command": "node",
      "args": ["/Users/eric/Documents/Cline/MCP/mcp-gsuite/build/index.js"],
      "env": {
        "GOOGLE_CLIENT_ID": "your_google_client_id",
        "GOOGLE_CLIENT_SECRET": "your_google_client_secret",
        "GOOGLE_REFRESH_TOKEN": "your_google_refresh_token"
      }
    },
    "calendar": {
      "command": "node",
      "args": ["/Users/eric/Documents/Cline/MCP/mcp-gsuite/build/index.js", "--service", "calendar"],
      "env": {
        "GOOGLE_CLIENT_ID": "your_google_client_id",
        "GOOGLE_CLIENT_SECRET": "your_google_client_secret",
        "GOOGLE_REFRESH_TOKEN": "your_google_refresh_token"
      }
    }
  }
}
